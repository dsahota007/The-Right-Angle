[sub_resource type="GDScript" id="GDScript_2vwy6"]
script/source = "extends TileMap


func _on_new_game_pressed():
	get_tree().change_scene_to_file(\"res://Scenes/world.tscn\")

#func _on_button_pressed():
	#get_tree().change_scene_to_file(\"res://Scenes/section_select.tscn\")

func _on_quitbtn_pressed():
	get_tree().quit()

func _ready():
	update_button_text()
	
 
@onready var vol = $vol

func _on_vol_pressed():
	GlobalAudio.toggle_volume()  
	update_button_text()

func update_button_text():
	if vol:
		if GlobalAudio.is_muted:
			vol.text = \" Turn Vol on \"
		else:
			vol.text = \"Turn Vol off\"
	else:
		print(\"Button node is not accessible\")
 
"

